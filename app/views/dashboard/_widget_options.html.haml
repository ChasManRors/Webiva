- if options && options.options_partial
  = cms_subfields_for 'widget[data]', options do |f|
    = render :partial => options.options_partial, :locals => options.options_locals(f)
  - if @widget_title
    :javascript
      if((!WidgetEditor.titleModified || $('widget_title').value=='') && $('widget_title'))  {
        $('widget_title').value = "#{jh @widget_title}";
      }
